{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyYears": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/yearmakemodel/year"
      },
      "backendUri": "https://www.carqueryapi.com/api/0.3/?cmd=getYears"
    },
    "proxyMakes": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/yearmakemodel/make?year={year}"
      },
      "backendUri": "https://www.carqueryapi.com/api/0.3/?cmd=getMakes&year={year}"
    },
    "proxyModels": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/yearmakemodel/model?year={year}&make={make}"
      },
      "backendUri": "https://www.carqueryapi.com/api/0.3/?cmd=getModels&make={make}&year={year}"
    },
  }
}